        "X_test, y_test = test[feature_cols].fillna(0), test['return_30d']\n",
        "\n",
        "# Train models\n",
        "dt = DecisionTreeClassifier(max_depth=6, min_samples_leaf=5, random_state=42)\n",
        "nb = GaussianNB()\n",
